{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "noEmit": true,
    "baseUrl": "./src",
    "paths": {
      "*": ["../*"],
      "src/*": ["./src/*"]
    }
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "./node_modules"
  ]
}
